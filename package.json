{
  "name": "duixml",
  "displayName": "Duilib skin xml tool",
  "publisher": "xiaofeng",
  "description": "Duilib皮肤编辑扩展，有问题请反馈到909049038@qq.com",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dui",
        "aliases": [
          "dui",
          "dui"
        ],
        "extensions": [
          "dui",
          "xml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dui",
        "scopeName": "source.dui",
        "path": "./syntaxes/dui.tmLanguage.json"
      },      
      {
        "language": "xml",
        "scopeName": "source.dui",
        "path": "./syntaxes/dui.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:dui",
    "onLanguage:xml"
  ],
  "main": "extension",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
      "url": "https://github.com/kbkb1216/DuiXml"
  },
  "author": "小峰同学",
  "license": "GPL",
  "dependencies": {
    "fs": "0.0.1-security",
    "xml2js": "^0.4.19"
  }
}
